        <requestFocus></requestFocus>
    </EditText>
    <EditText android:layout_height="wrap_content" android:layout_width="250dip" android:layout_marginLeft="5dip" android:layout_marginRight="5dip" android:id="@+id/txtSearch" android:singleLine="true" android:hint="Search Artist" android:background="@drawable/search_bar" android:paddingLeft="5dip" android:paddingRight="5dip"></EditText>
</RelativeLayout>

<LinearLayout
